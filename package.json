{
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@playwright/test": "next",
		"@storybook/addon-actions": "^6.0.0",
		"@storybook/addon-essentials": "^6.0.0",
		"@storybook/addon-links": "^6.0.0",
		"@storybook/builder-webpack4": "^6.0.0",
		"@storybook/manager-webpack4": "^6.0.0",
		"@storybook/svelte": "^6.0.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "5.18.0",
		"@typescript-eslint/parser": "5.18.0",
		"babel-loader": "^8.2.4",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-storybook": "^0.5.8",
		"eslint-plugin-svelte3": "3.4.1",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.7",
		"prettier": "2.6.2",
		"prettier-plugin-svelte": "2.7.0",
		"svelte": "^3.47.0",
		"svelte-check": "2.6.0",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "4.10.5",
		"tslib": "2.3.1",
		"typescript": "4.6.3",
		"vite-plugin-windicss": "^1.8.3",
		"windicss": "^3.5.1"
	},
	"lint-staged": {
		"*.{js, ts, svelte}": [
			"eslint --cache --fix"
		],
		"*.{svelte, html, css, scss, js, ts, json, yml, yaml, md}": [
			"prettier --write --plugin-search-dir=."
		]
	},
	"name": "my-app",
	"prettier": {
		"singleQuote": true,
		"tabWidth": 4,
		"useTabs": true
	},
	"scripts": {
		"build": "svelte-kit build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "svelte-kit dev",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"package": "svelte-kit package",
		"prepare": "svelte-kit sync && husky install && npx playwright install",
		"preview": "svelte-kit preview",
		"test": "playwright test",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"type": "module",
	"version": "0.0.1",
	"volta": {
		"node": "16.14.2"
	},
	"dependencies": {}
}
